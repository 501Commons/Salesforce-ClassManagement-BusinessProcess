        <dataType>String</dataType>
        <expression>IF ({!soContact.c501_cm__C501_PBC_Cert_Remaining_Days__c} &lt;= 0,
&quot;Expired&quot;,
TEXT({!soContact.c501_cm__C501_PBC_Cert_Remaining_Days__c}) + &quot; Day(s) Remaining&quot;)</expression>
    </formulas>
    <formulas>
        <name>getSafeSleepDaysRemaining</name>
        <dataType>String</dataType>
        <expression>IF ({!soContact.c501_cm__C501_Safe_Sleep_Cert_Remaining_Days__c} &lt;= 0,
&quot;Expired&quot;,
TEXT({!soContact.c501_cm__C501_Safe_Sleep_Cert_Remaining_Days__c}) + &quot; Day(s) Remaining&quot;)</expression>
    </formulas>
    <formulas>
        <name>isExpired</name>
        <dataType>Boolean</dataType>
        <expression>({!soContact.c501_cm__C501_Cpr_Cert_Days_Remaining__c} &lt;= 0 ||
{!soContact.c501_cm__C501_First_Aid_Cert_Days_Remaining__c} &lt;= 0 ||
{!soContact.c501_cm__C501_PBC_Cert_Remaining_Days__c} &lt;= 0 ||
{!soContact.c501_cm__C501_Safe_Sleep_Cert_Remaining_Days__c} &lt;= 0 ||
{!soContact.c501_cm__C501_Food_Handler_Cert_Remaining_Days__c} &lt;= 0)</expression>
    </formulas>
