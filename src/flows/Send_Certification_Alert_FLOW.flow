    <formulas>
        <name>buildSMSMessage</name>
        <dataType>String</dataType>
        <expression>&quot;Hey, this is the Imagine Institute! Your credentials are about to expire or are expired! &quot; +
{!getCredsExpiredMessageTextOnly} + 
&quot;  &gt;&gt;Change your preferences here: https://imaginewa.org/pref&quot;</expression>
    </formulas>
    <formulas>
        <name>getCprDaysRemaining</name>
        <dataType>String</dataType>
        <expression>IF ({!soContact.c501_cm__C501_Cpr_Cert_Days_Remaining__c} &lt;= 0,
&quot;Expired&quot;, 
TEXT({!soContact.c501_cm__C501_Cpr_Cert_Days_Remaining__c}) + &quot; Day(s) Remaining&quot;)</expression>
    </formulas>
    <formulas>
        <name>getCredsExpiredMessage</name>
        <dataType>String</dataType>
        <expression>IF ({!soContact.c501_cm__C501_Cpr_Cert_Days_Remaining__c} = 30 || {!soContact.c501_cm__C501_Cpr_Cert_Days_Remaining__c} = 14 ||  {!soContact.c501_cm__C501_Cpr_Cert_Days_Remaining__c} &lt;= 2,
&quot;&lt;b&gt;CPR&lt;/b&gt;&lt;ul&gt;&lt;li&gt;Certification &quot; 
  + TEXT(DATEVALUE({!soContact.c501_cm__ii_Cpr_Cert_Date__c})) + &quot;&lt;/li&gt;&lt;li&gt;Status: &quot; + {!getCprDaysRemaining} + &quot;&lt;/li&gt;&lt;/ul&gt;&quot;,&quot;&quot;)
+
IF ({!soContact.c501_cm__C501_First_Aid_Cert_Days_Remaining__c} = 30 || {!soContact.c501_cm__C501_First_Aid_Cert_Days_Remaining__c} = 14 ||  {!soContact.c501_cm__C501_First_Aid_Cert_Days_Remaining__c} &lt;= 2,
&quot;&lt;b&gt;First Aid&lt;/b&gt;&lt;li&gt;&lt;ul&gt;Certification &quot; 
  + TEXT(DATEVALUE({!soContact.c501_cm__ii_First_Aid_Cert_Date__c})) + &quot;&lt;/li&gt;&lt;li&gt;Status: &quot; + {!getFirstAidDaysRemaining} + &quot;&lt;/li&gt;&lt;li&gt;Update by signing up for a free training on our website @ http://imaginewa.org/become-a-substitute/&lt;/li&gt;&lt;/ul&gt;&quot;,&quot;&quot;)
+
IF ({!soContact.c501_cm__C501_PBC_Cert_Remaining_Days__c} = 30 || {!soContact.c501_cm__C501_PBC_Cert_Remaining_Days__c} = 14 ||  {!soContact.c501_cm__C501_PBC_Cert_Remaining_Days__c} &lt;= 2,
&quot;&lt;b&gt;Portable Background Check (PBC)&lt;/b&gt;&lt;li&gt;&lt;ul&gt;Certification &quot; 
  + TEXT(DATEVALUE({!soContact.c501_cm__ii_PBC_Expiration__c})) + &quot;&lt;/li&gt;&lt;li&gt;Status: &quot; + {!getPbcRemainingDays} + &quot;&lt;/li&gt;&lt;li&gt;Update in MERIT @ https://apps.del.wa.gov/MERIT&lt;/li&gt;&lt;/ul&gt;&quot;,&quot;&quot;)
+
IF ({!soContact.c501_cm__C501_Safe_Sleep_Cert_Remaining_Days__c} = 30 || {!soContact.c501_cm__C501_Safe_Sleep_Cert_Remaining_Days__c} = 14 ||  {!soContact.c501_cm__C501_Safe_Sleep_Cert_Remaining_Days__c} &lt;= 2,
&quot;&lt;b&gt;Infant Safe Sleep&lt;/b&gt;&lt;li&gt;&lt;ul&gt;Certification &quot; 
  + TEXT(DATEVALUE({!soContact.c501_cm__ii_Safe_Sleep_Cert_Date__c})) + &quot;&lt;/li&gt;&lt;li&gt;Status: &quot; + {!getSafeSleepDaysRemaining} + &quot;&lt;/li&gt;&lt;li&gt;Update @ http://deltraining.com/&lt;/li&gt;&lt;/ul&gt;&quot;,&quot;&quot;)
+
IF ({!soContact.c501_cm__C501_Food_Handler_Cert_Remaining_Days__c} = 30 || {!soContact.c501_cm__C501_Food_Handler_Cert_Remaining_Days__c} = 14 ||  {!soContact.c501_cm__C501_Food_Handler_Cert_Remaining_Days__c} &lt;= 2,
&quot;&lt;b&gt;Food Handler&lt;/b&gt;&lt;li&gt;&lt;ul&gt;Certification &quot; 
  + TEXT(DATEVALUE({!soContact.c501_cm__ii_Food_Handler_Cert_Date__c})) + &quot;&lt;/li&gt;&lt;li&gt;Status: &quot; + {!getFoodHandlerDaysRemaining} + &quot;&lt;/li&gt;&lt;li&gt;Update @ https://www.doh.wa.gov/CommunityandEnvironment/Food/FoodWorkerandIndustry/FoodWorkerCard&lt;/li&gt;&lt;/ul&gt;&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>getCredsExpiredMessageTextOnly</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(
SUBSTITUTE(
SUBSTITUTE(
SUBSTITUTE(
SUBSTITUTE(
SUBSTITUTE({!getCredsExpiredMessage}, &quot;&lt;b&gt;&quot;, &quot;&quot;), &quot;&lt;/b&gt;&quot;, &quot;&quot;), &quot;&lt;ul&gt;&quot;, &quot;&quot;), &quot;&lt;/ul&gt;&quot;, &quot;&quot;), &quot;&lt;li&gt;&quot;, &quot; &quot;), &quot;&lt;/li&gt;&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>getDaysSinceLastCertificationEvent</name>
        <dataType>Number</dataType>
        <expression>{!$Flow.CurrentDateTime} - {!lastEventCreatedDate}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
