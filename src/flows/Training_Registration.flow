    <formulas>
        <name>getCompanyName</name>
        <dataType>String</dataType>
        <expression>IF(LEN({!varInputDay1Filter}) &gt; 0,
    {!varInputDay1Filter},
    IF(LEN({!Substitute_Last_Name}) &gt; 0, 
