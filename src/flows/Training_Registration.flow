    <formulas>
        <name>getTrainingSummary</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!soDay1Training.Name}), &quot;&quot;, &quot;Day1 Training: &quot; + {!soDay1Training.Name} + &quot;&lt;br/&gt;&quot;) +
